{
  "name": "rizzui-pkg",
  "version": "0.1.9",
  "description": "A demo package for testing",
  "keywords": [
    "demo",
    "typescript",
    "rizzui-pkg"
  ],
  "homepage": "https://github.com/md-nahid/test-npm-pkg",
  "bugs": {
    "url": "https://github.com/md-nahid/test-npm-pkg/issues"
  },
  "author": "Nahid",
  "repository": {
    "type": "git",
    "url": "https://github.com/md-nahid/test-npm-pkg.git"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:rollup": "rollup -c",
    "dev:test": "vitest",
    "test": "vitest run",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "release": "changeset version && changeset publish",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "ci": "pnpm run build && pnpm run check-format && pnpm run check-exports && pnpm run test"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "react-number-format": "5.4.2",
    "tailwind-merge": "2.5.4"
  },
  "peerDependencies": {
    "@floating-ui/react": ">=0.25.4",
    "@headlessui/react": ">=2.0.3",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@floating-ui/react": "^0.26.27",
    "@headlessui/react": "^2.2.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/react": "^18.3.12",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.24.4",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "tailwindcss": "^3.4.14",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
